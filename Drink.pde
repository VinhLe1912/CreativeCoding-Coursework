

public class Drink{
  public int hour;
  int minute;
  String drink;
  
  public Drink(){}
 
  public Drink(int hour, int minute, String drink){
    this.hour = hour;
    this.minute = minute;
    this.drink = drink;
    
  }
  
  
  
  public int getHour(){return hour;}
  public int getMinute(){return minute;}
  public String getDrink(){return drink;};
  
  
}
  
